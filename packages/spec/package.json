{
  "name": "@agent-sdk/spec",
  "version": "0.1.0",
  "description": "AgentSDK JSON Schema specification and validation utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "rm -rf dist",
    "validate": "ajv validate -s src/schemas/agent-sdk.schema.json -d '../../sdks/*/agent-sdk.json'"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "jest": "^29.0.0"
  },
  "files": [
    "dist/",
    "src/schemas/"
  ],
  "keywords": ["json-schema", "validation", "agent-sdk"],
  "license": "MIT"
}
